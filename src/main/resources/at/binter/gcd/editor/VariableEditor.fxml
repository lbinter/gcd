<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TitledPane fx:id="editorTitle" animated="false" collapsible="false" text="%editor.variable.edit.title"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="at.binter.gcd.controller.VariableEditorController">
    <content>
        <AnchorPane>
            <children>
                <VBox>
                    <children>
                        <GridPane fx:id="grid">
                            <columnConstraints>
                                <ColumnConstraints halignment="RIGHT" hgrow="NEVER"/>
                                <ColumnConstraints hgrow="NEVER" maxWidth="475.0" minWidth="475.0" prefWidth="475.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="0.0" prefHeight="0.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="0.0" prefHeight="0.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                                <RowConstraints/>
                            </rowConstraints>
                            <children>
                                <Label text="%editor.label.name" GridPane.rowIndex="1">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label fx:id="editorLabelName" minWidth="360" GridPane.columnIndex="1"
                                       GridPane.rowIndex="1">
                                    <GridPane.margin>
                                        <Insets left="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="%editor.label.description">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="%editor.label.initial.condition" GridPane.rowIndex="2">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <TextArea fx:id="editorInitialCondition" minWidth="360.0" prefRowCount="2"
                                          GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.rowSpan="2">
                                    <opaqueInsets>
                                        <Insets bottom="2.0"/>
                                    </opaqueInsets>
                                </TextArea>
                                <Label text="%editor.label.value.start" GridPane.rowIndex="6">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="editorValueStart" minWidth="360" GridPane.columnIndex="1"
                                           GridPane.rowIndex="6"/>
                                <Label text="%editor.label.value.minimum" GridPane.rowIndex="7">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="editorValueMinimum" minWidth="360" GridPane.columnIndex="1"
                                           GridPane.rowIndex="7"/>
                                <Label text="%editor.label.value.maximum" GridPane.rowIndex="8">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="editorValueMaximum" minWidth="360" GridPane.columnIndex="1"
                                           GridPane.rowIndex="8"/>
                                <Label text="%editor.label.algebraicVariables" GridPane.rowIndex="9">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label fx:id="editorLabelAlgebraicVariables" minWidth="360" GridPane.columnIndex="1"
                                       GridPane.rowIndex="9">
                                    <GridPane.margin>
                                        <Insets left="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="%editor.label.agents" GridPane.rowIndex="10">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label fx:id="editorLabelAgents" minWidth="360" GridPane.columnIndex="1"
                                       GridPane.rowIndex="10">
                                    <GridPane.margin>
                                        <Insets left="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="%editor.label.constraints" GridPane.rowIndex="11">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label fx:id="editorLabelConstraints" minWidth="360" GridPane.columnIndex="1"
                                       GridPane.rowIndex="11">
                                    <GridPane.margin>
                                        <Insets left="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="%editor.label.plot.color" GridPane.rowIndex="12">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="editorPlotColor" minWidth="360" GridPane.columnIndex="1"
                                           GridPane.rowIndex="12"/>
                                <Label text="%editor.label.plot.thickness" GridPane.rowIndex="13">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="editorPlotThickness" minWidth="360" GridPane.columnIndex="1"
                                           GridPane.rowIndex="13"/>
                                <Label text="%editor.label.plot.line.style" GridPane.rowIndex="14">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="editorPlotLineArt" minWidth="360" GridPane.columnIndex="1"
                                           GridPane.rowIndex="14"/>
                                <TextField fx:id="editorDescription" minWidth="360" GridPane.columnIndex="1"/>
                                <Label fx:id="editorLabelVariable" text="%editor.label.variables" visible="false"
                                       GridPane.rowIndex="4">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label fx:id="editorLabelVariables" minWidth="360" visible="false"
                                       GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                                <Label fx:id="editorLabelParameter" text="%editor.label.parameters" visible="false"
                                       GridPane.rowIndex="5">
                                    <GridPane.margin>
                                        <Insets left="10.0" right="5.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label fx:id="editorLabelParameters" minWidth="360" visible="false"
                                       GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                                <Button fx:id="transformButton" mnemonicParsing="false" onAction="#transformFunction"
                                        text="%editor.label.function.transform" GridPane.rowIndex="3">
                                    <GridPane.margin>
                                        <Insets right="3.0"/>
                                    </GridPane.margin>
                                </Button>
                            </children>
                        </GridPane>
                        <HBox alignment="CENTER">
                            <children>
                                <Button fx:id="editorButtonConfirm" minWidth="100.0" mnemonicParsing="false"
                                        text="%editor.button.edit"/>
                                <Separator opacity="0.0" orientation="VERTICAL" prefHeight="0.0">
                                    <HBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </HBox.margin>
                                </Separator>
                                <Button fx:id="editorButtonCancel" minWidth="100.0" mnemonicParsing="false"
                                        text="%editor.button.cancel"/>
                            </children>
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </HBox>
                    </children>
                    <padding>
                        <Insets top="10.0"/>
                    </padding>
                </VBox>
            </children>
        </AnchorPane>
    </content>
</TitledPane>
